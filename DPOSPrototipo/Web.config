<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
    <system.webServer>
        <defaultDocument>
            <files>
                <add value="Login.aspx" />
            </files>
        </defaultDocument>
    </system.webServer>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
            multipleSiteBindingsEnabled="true" />
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IUsuarios" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:25097/Usuarios.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUsuarios" contract="UsuariosWS.IUsuarios"
                name="BasicHttpBinding_IUsuarios" />
        </client>
    </system.serviceModel>
</configuration>